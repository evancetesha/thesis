#!/bin/bash

#Clean
docker kill my-running-app
docker rm my-running-app

# Build Container
docker build -t test_webserver .

# Run Container
docker run -dit --name my-running-app -p 8080:80 test_webserver

#Make dummy request
curl http://127.0.0.1:8080

#Stop Container
docker export -o container.tar my-running-app

